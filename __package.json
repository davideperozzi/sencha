{
  "name": "sencha",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "start": "bun run ./src/index.ts",
    "build": "rimraf dist && npm run build:esm && npm run build:cjs",
    "build:esm": "swc src -d dist && tsc --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.66",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.3.1",
    "@types/pug": "^2.0.6",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@fastify/deepmerge": "^1.3.0",
    "chokidar": "^3.5.3",
    "fs-extra": "^11.1.1"
  },
  "optionalDependencies": {
    "pug": "^3.0.2",
    "sass": "^1.63.6",
    "eta": "^3.0.3",
    "esbuild": "^0.18.6",
    "postcss": "^8.4.24",
    "lightningcss": "^1.21.0"
  }
}
